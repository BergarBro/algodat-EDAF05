import java.util.Scanner;

public class test {
	
	public static void main(String[] args) {
		int s = 30;
		Heap hej = new Heap(s);
		Scanner scan = new Scanner(System.in);
	
		for (int i = 0; i < s; i++) {
			Vertex temp = new Vertex(i);
			temp.setDist(scan.nextInt());
			hej.add(temp);
		}
		scan.close();
		for (int i = 0; i < s; i++) {
			System.out.print(hej.getSmall().getDist());
		}
	}
}
